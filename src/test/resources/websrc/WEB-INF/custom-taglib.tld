<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
	"http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>

  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>custom-taglib</short-name>
  <uri>http://jsptest.sf.net/taglib</uri>
  <description>taglib example</description>

  <tag>
    <name>date</name>
    <tag-class>net.sf.jsptest.acceptance.jsp.CustomTag</tag-class>
    <body-content>TAGDEPENDENT</body-content>
    <description>Render request information</description>
    <attribute>
       <name>tz</name>
       <required>false</required>
    </attribute>
  </tag>       

</taglib>
